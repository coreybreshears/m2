div.table-container-white {
	height: 375px;
	overflow: auto;
	width:100%;
	border:1px solid #d7dbde;
	background:#FEFEFE;
	font:14px/18px 'Open Sans', Arial, Helvetica, sans-serif;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	color:#303233;
}
.search-form-button, .search-form-button-invoice-details {
    display:inline-block;
    float: right;
    margin: 4px 11px 2px 0;
    min-width:120px;
    font:600 13px/16px 'Open Sans', Arial, Helvetica, sans-serif;
    height:23px;
    text-transform:uppercase;
    text-align: center;
    border-radius:3px;
    border:none;
    cursor:pointer;
    background: #dee0e0;
    color:#303233 !important;
}
.search-form-button-invoice-details{
    padding-top: 7px;
    padding-left: 7px;
    padding-right: 7px;
    font-size: 13px !important;
}
.search-form-button[status="up"]{
	padding-right: 20px;
}
.search-form-button[status="up"]:after{
	content:'';
	position: absolute;
	height:11px;
	width:7px;
	margin-left: 7px;
	margin-top: 3px;
	background:url(../images/m2_images/sprite.png) no-repeat -147px -8px;
	z-index:5;
	-moz-transform:rotate(90deg);
	-webkit-transform:rotate(90deg);
	transform:rotate(90deg);
}
.search-form-button[status="down"]{
	padding-right: 20px;
}
.search-form-button[status="down"]:after{
	content:'';
	position: absolute;
	height:9px;
	width:7px;
	margin-left: 7px;
	margin-top: 4px;
	background:url(../images/m2_images/sprite.png) no-repeat -148px -10px;
	z-index:5;
	-moz-transform:rotate(-90deg);
	-webkit-transform:rotate(-90deg);
	transform:rotate(-90deg);
}
.create-form-button {
    display:inline-block;
    float: right;
    margin: 4px 11px 4px 0;
    min-width:120px;
    font:600 13px/16px 'Open Sans', Arial, Helvetica, sans-serif;
    height:27px;
    text-transform:uppercase;
    text-align: center;
    border-radius:3px;
    border:none;
    cursor:pointer;
    background: #dee0e0;
    color:#303233;
}
.create-form-button[status="up"]{
    padding-right: 20px;
}
.create-form-button[status="up"]:after{
    content:'';
    position: absolute;
    height:11px;
    width:7px;
    margin-left: 7px;
    margin-top: 3px;
    background:url(../images/m2_images/sprite.png) no-repeat -147px -8px;
    z-index:5;
    -moz-transform:rotate(90deg);
    -webkit-transform:rotate(90deg);
    transform:rotate(90deg);
}
.create-form-button[status="down"]{
    padding-right: 20px;
}
.create-form-button[status="down"]:after{
    content:'';
    position: absolute;
    height:9px;
    width:7px;
    margin-left: 7px;
    margin-top: 4px;
    background:url(../images/m2_images/sprite.png) no-repeat -148px -10px;
    z-index:5;
    -moz-transform:rotate(-90deg);
    -webkit-transform:rotate(-90deg);
    transform:rotate(-90deg);
}
