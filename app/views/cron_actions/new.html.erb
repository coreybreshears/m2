<%= form_for(:cron_setting, url: {action: 'create'}) do |f| %>
  <%= render 'form',
              f: f,
              cron_setting: @cron_setting,
              users: @users,
              tariffs: @tariffs,
              email_sending_disabled: @email_sending_disabled
  %>
    <%= f.submit _('Create'), :disable_with => _('Processing..') %>
<% end %>