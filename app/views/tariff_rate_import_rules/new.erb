<div class="content-box">
  <div class="titled-box">
    <h2><%= _('New_Rate_Import_Rules') %></h2>
    <%= form_for :tariff_rate_import_rule, url: {action: :create, id: @tariff_rate_import_rule}, html: {class: 'details-form'} do |f| %>
        <fieldset>
          <%= render 'form', f: f, tariff_rate_import_rule: @tariff_rate_import_rule %>
          <br/><br/>
          <div class="buttons-row">
            <%= submit_tag _('CANCEL'), type: :reset, onclick: "location.href = '#{Web_Dir}/tariff_rate_import_rules/list'" %>
            <%= submit_tag _('CREATE'), disable_with: _('Processing..') %>
          </div>
        </fieldset>
    <% end %>
  </div>
</div>