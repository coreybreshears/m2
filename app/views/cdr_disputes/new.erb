<div class="content-box">
  <div class="table-box">
    <h2><%= _('New_CDR_Dispute') %></h2>
    <%= form_for @dispute, url: {action: :create}, html: {class: 'details-form'} do |f| %>
        <%= render 'form',  f: f, currencies: @currencies, templates: @templates, dispute: @dispute %>
        <div class="buttons-row">
          <%= f.submit _('Cancel'), name: 'cancel', type: 'reset', onclick: "location.href = '#{Web_Dir}/cdr_disputes/list'" %>
          <%= f.submit _('Save_and_proceed_to_import'), disable_with: 'Processing...', style: 'width: auto;padding:0 12px;' %>
        </div>
    <% end %>
  </div>
</div>